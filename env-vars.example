### Authentication details
export TF_VAR_tenancy_ocid="<tenant OCID>"
export TF_VAR_user_ocid="<user OCID>"
export TF_VAR_fingerprint="<api key fingerprint>"
export TF_VAR_private_key_path="<path/to/api/pem/file>"

### Compartment
export TF_VAR_compartment_ocid="<compartment OCID>"

### Public/private keys used on the instance
export TF_VAR_ssh_username=ubuntu
export TF_VAR_ssh_public_key=$(cat ./terraform-ssh.pub)
export TF_VAR_ssh_private_key=$(cat ./terraform-ssh.pem)
